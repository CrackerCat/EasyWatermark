<resources xmlns:android="http://schemas.android.com/apk/res/android">


    <style name="MaterialAlertDialog.App" parent="MaterialAlertDialog.Material3">
        <item name="shapeAppearance">@style/ShapeAppearance.App.MediumComponent</item>
    </style>

    <style name="MaterialAlertDialog.App.Title.Text" parent="MaterialAlertDialog.Material3.Title.Text">
<!--        <item name="android:textColor">@color/d_brand</item>-->
    </style>

    <style name="ThemeOverlay.App.Button" parent="">
        <!--        <item name="colorPrimary">@color/shrine_pink_900</item>-->
    </style>

    <style name="ShapeAppearance.App.MediumComponent" parent="ShapeAppearance.Material3.MediumComponent">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">8dp</item>
    </style>

    <style name="ShapeAppearance.App.SmallComponent" parent="ShapeAppearance.Material3.SmallComponent">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">4dp</item>
    </style>

    <style name="ShapeAppearance.App.Button" parent="ShapeAppearance.Material3.MediumComponent">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">0dp</item>
    </style>

    <style name="WindowAnimationTransition" parent="Animation.MaterialComponents.BottomSheetDialog">
        <item name="android:activityOpenEnterAnimation">@anim/activity_open_in</item>
        <item name="android:activityOpenExitAnimation">@anim/activity_open_out</item>
        <item name="android:activityCloseEnterAnimation">@anim/activity_close_in</item>
        <item name="android:activityCloseExitAnimation">@anim/activity_close_out</item>
    </style>

    <style name="Theme.MyApp.ActionOverflowButtonStyle">
        <item name="android:src">@drawable/ic_more</item>
    </style>

    <style name="Theme.MyApp.MaterialAboutActivity" parent="Theme.MyApp">
        <item name="android:textSize">14sp</item>
        <item name="android:windowAnimationStyle">@style/WindowAnimationTransition</item>
    </style>

    <style name="Theme.MyApp.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar">
        <!--        <item name="android:textColorPrimary">@color/text_color_main</item>-->
    </style>

    <style name="Theme.MyApp.TitleText" parent="Theme.MyApp">
        <item name="android:textSize">14sp</item>
    </style>

    <style name="Theme.MyApp.TipsText" parent="Theme.MyApp">
        <item name="android:layout_marginTop">3dp</item>
        <item name="android:textSize">12sp</item>
        <!--        <item name="android:textColor">@color/text_color_tips</item>-->
    </style>

    <style name="MySeekBarStyle">
        <item name="android:maxHeight">20dp</item>
        <item name="android:height">20dp</item>
<!--        <item name="android:progressBackgroundTint">@color/white_14</item>-->
<!--        <item name="android:progressTint">@color/white</item>-->
<!--        <item name="android:colorControlActivated">@color/white</item>-->
        <item name="android:backgroundTint">@android:color/transparent</item>
<!--        <item name="thumbColor">@color/white</item>-->
        <item name="thumbRadius">10dp</item>
        <item name="thumbStrokeColor">@android:color/transparent</item>
        <item name="tickColor">@android:color/transparent</item>
<!--        <item name="trackColor">@color/d_slide_bg</item>-->
<!--        <item name="trackColorActive">@color/colorAccent</item>-->
<!--        <item name="trackColorInactive">@color/colorAccent</item>-->
        <item name="trackHeight">2dp</item>
    </style>

    <style name="DialogButtonStyle" parent="Widget.Material3.Button.TextButton.Dialog">
<!--        <item name="android:textColor">@color/colorAccent</item>-->
<!--        <item name="color">@color/text_color_main</item>-->
    </style>

    <style name="ThemeOverlay.Material3.Dialog.MyDialog" parent="ThemeOverlay.Material3.Dialog">
        <item name="materialButtonStyle">@style/DialogButtonStyle</item>
        <item name="android:background">?attr/colorPrimaryDark</item>
    </style>

    <style name="Widget.App.TabLayout" parent="Widget.Material3.TabLayout">
<!--        <item name="tabTextColor">@color/selector_tab_color</item>-->
<!--        <item name="tabIconTint">@color/selector_tab_color</item>-->
        <item name="tabTextAppearance">@style/CustomTextAppearanceTab</item>
    </style>

    <style name="CustomTextAppearanceTab" parent="@android:style/TextAppearance">
        <item name="textAllCaps">false</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="Theme.MyApp.EditTextDialogOverlay">
        <item name="colorPrimary">?attr/colorPrimary</item>
        <item name="colorPrimaryDark">?attr/colorPrimary</item>
        <item name="colorAccent">?attr/colorPrimary</item>
        <!--        <item name="android:textColor">@color/text_color_main</item>-->
        <!--        <item name="colorOnSurface">@color/white</item>-->
    </style>

    <style name="TextInputLayoutStyle" parent="Widget.Material3.TextInputLayout.OutlinedBox">
        <item name="boxStrokeWidth">0dp</item>
        <item name="boxStrokeWidthFocused">0dp</item>
    </style>

    <style name="TextInputCursorStyle" parent="ThemeOverlay.Material3.TextInputEditText.OutlinedBox">
<!--        <item name="colorControlActivated">@color/colorAccent</item>-->
    </style>

    <style name="ProgressBarStyle" parent="Widget.AppCompat.ProgressBar">
        <item name="android:indeterminate">true</item>
<!--        <item name="android:indeterminateTint">@color/white</item>-->
<!--        <item name="android:progressBackgroundTint">@color/white</item>-->
    </style>

    <style name="MaterialAlertDialog" parent="ThemeOverlay.Material3.MaterialAlertDialog">
        <item name="buttonBarPositiveButtonStyle">@style/Widget.App.Dialog.Button</item>
<!--        <item name="colorOnSurface">@color/white</item>-->
        <item name="buttonBarNegativeButtonStyle">@style/Widget.App.Dialog.Button</item>
    </style>

    <style name="Widget.App.Dialog.Button" parent="Widget.Material3.Button.TextButton.Dialog">
<!--        <item name="android:textColor">@color/white</item>-->
    </style>

    <style name="Widget.App.OutlineButton" parent="Widget.Material3.Button.OutlinedButton">
        <item name="materialThemeOverlay">@style/ThemeOverlay.App.Button.TextOutlineButton</item>
        <item name="android:paddingTop">16dp</item>
        <item name="android:paddingBottom">16dp</item>
        <item name="shapeAppearance">@style/ShapeAppearance.App.Button</item>
    </style>

    <style name="ThemeOverlay.App.Button.TextOutlineButton" parent="">
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="ThemeOverlay.App.Button.TextButton" parent="">
        <item name="android:textSize">16sp</item>
    </style>


    <style name="AboutPageTextViewStyle" parent="TextAppearance.Material3.TitleMedium">
        <item name="android:paddingStart">28dp</item>
        <item name="android:paddingEnd">28dp</item>
    </style>

    <style name="SaveDialogTextViewStyle" parent="Widget.MaterialComponents.BottomSheet.Modal">
        <item name="android:textColor">@color/md_theme_dark_onSurface</item>
        <item name="android:paddingStart">0dp</item>
        <item name="android:paddingEnd">0dp</item>
    </style>

    <style name="SaveDialogExposedDropdownMenu" parent="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.ExposedDropdownMenu">
        <item name="boxBackgroundColor">@color/md_theme_dark_surface</item>
        <item name="boxStrokeColor">@color/md_theme_dark_outline</item>
        <item name="shapeAppearance">@style/ShapeAppearance.App.SmallComponent</item>
        <item name="hintTextColor">@color/md_theme_dark_onSurface</item>
        <item name="endIconTint">@color/md_theme_dark_onSurface</item>
        <item name="android:textColor">@color/md_theme_dark_onSurface</item>
        <item name="android:textColorHint">@color/md_theme_dark_onSurface</item>
    </style>

    <style name="SaveDialogAutoCompleteTextView">
        <item name="hintTextColor">@color/md_theme_dark_onSurface</item>
        <item name="android:textColor">@color/md_theme_dark_onSurface</item>
        <item name="android:textColorHint">@color/md_theme_dark_onSurface</item>
    </style>

</resources>